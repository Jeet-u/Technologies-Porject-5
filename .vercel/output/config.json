{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/contact\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/contact\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/favicon\\.ico\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/insights(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/manifest\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/products\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/robots\\.txt\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/services\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}